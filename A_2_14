import java.util.Scanner;
public class A_2_14{

    public static void main(String[] args) {
        try (Scanner sc = new Scanner (System.in)) {
            int n = sc.nextInt();
            if (n>=1 &&n<=10 ){
                for(int i=1;i<=10;i++){
                    System.out.println(n+"*"+i+"="+n*i);
                }
            }
        }
    }
}
